{
	"name": "idems_sourcing_suite",
	"version": "1.0.0",
	"description": "A collection of utilities on various sites to make sourcing for e621 easier",
	"scripts": {
		"build": "node build.js"
	},
	"author": "",
	"license": "UNLICENSE",
	"private": true,
	"devDependencies": {
		"eslint": "^6.8.0",
		"eslint-config-standard": "^14.1.0",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-node": "^11.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.1",
		"browserify": "^16.5.0",
		"stringify": "^5.2.0"
	},
	"eslintConfig": {
		"extends": "standard",
		"parserOptions": {
			"ecmaVersion": 9,
			"sourceType": "module"
		},
		"env": {
			"browser": true
		},
		"rules": {
			"semi": [
				"error",
				"always"
			],
			"no-console": "off",
			"indent": [
				"error",
				"tab",
				{
					"SwitchCase": 1,
					"ignoreComments": true
				}
			],
			"no-tabs": "off",
			"camelcase": "off"
		}
	},
	"dependencies": {},
	"main": "main.js",
	"eslintIgnore": [
		"distribution/*"
	]
}
